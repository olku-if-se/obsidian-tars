{
	"name": "obsidian-tars",
	"version": "3.5.0",
	"description": "Text generation based on tag suggestions, using DeepSeek, Claude, OpenAI, OpenRouter, SiliconFlow, Gemini, Qwen & more.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "./scripts/build.sh",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint": "biome lint .",
		"format": "biome format --write .",
		"check": "biome check --write .",
		"typecheck:build": "tsc --project tsconfig.build.json --noEmit --skipLibCheck",
		"typecheck:tests": "tsc --project tsconfig.tests.json --noEmit",
		"test": "vitest run",
		"test:coverage": "vitest run --coverage",
		"test:watch": "vitest --ui --coverage --watch",
		"clean": "rm -rf dist node_modules coverage .turbo"
	},
	"keywords": [],
	"author": "C Jack<https://github.com/ae86jack>",
	"license": "MIT",
	"devDependencies": {
		"@anthropic-ai/sdk": "^0.65.0",
		"@biomejs/biome": "2.2.6",
		"@google/generative-ai": "^0.24.1",
		"@types/node": "^24.6.1",
		"@types/react": "^18.3.12",
		"@types/react-dom": "^18.3.1",
		"@types/ws": "^8.18.1",
		"@vitest/coverage-v8": "^3.2.4",
		"@vitest/ui": "^3.2.4",
		"axios": "^1.8.3",
		"builtin-modules": "5.0.0",
		"esbuild": "^0.25.10",
		"handlebars": "^4.7.8",
		"jose": "^6.1.0",
		"jsdom": "^27.0.0",
		"obsidian": "1.10.0",
		"ollama": "^0.6.0",
		"openai": "^6.3.0",
		"testcontainers": "^11.7.0",
		"tslib": "^2.4.0",
		"typescript": "^5.9.3",
		"vitest": "^3.2.4"
	},
	"dependencies": {
		"@modelcontextprotocol/sdk": "^1.18.2",
		"@needle-di/core": "^1.0.0",
		"@tars/contracts": "workspace:*",
		"@tars/logger": "workspace:*",
		"@tars/mcp-hosting": "workspace:*",
		"@tars/providers": "workspace:*",
		"@tars/streams": "workspace:*",
		"@tars/ui": "workspace:*",
		"@types/debug": "^4.1.12",
		"async-mutex": "^0.5.0",
		"debug": "^4.4.3",
		"mcp-use": "^1.0.0",
		"p-limit": "^7.1.1",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"reflect-metadata": "^0.1.13",
		"yaml": "^2.8.1"
	}
}
